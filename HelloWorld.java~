public class HelloWorld {
     public static void main(String[] args) {
        int a = 99;
        while (a>=1) {
          System.out.println(a+" bottles of beer on the wall, " +a+"bottles of beer. Take one down, pass it around, ");
          a--;
          System.out.println(a+ "bottles of beer on the wall.");
        }
      }
    }
                              